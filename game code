        projectile.setFlag(SpriteFlag.GhostThroughWalls, true)
    }
})
game.onUpdateInterval(60000, function () {
    if (day == true) {
        story.startConveration(function () {
            story.printCharacterText("it is night")
            story.printCharacterText("wold you like to sleep")
            story.showPlayerChoices("yes", "no")
            if (story.checkLastAnswer("yes")) {
                story.spriteMoveToTile(mouse_selector, tiles.getTileLocation(7, 10), 50)
                day = false
                multilights.toggleLighting(false)
                lantern.stopLanternEffect()
                story.cancelCurrentConversation()
            } else {
                story.printCharacterText("O.K.")
                story.cancelCurrentConversation()
            }
        })
        multilights.toggleLighting(false)
        day = false
        lantern.startLanternEffect(mouse_selector)
    } else {
        multilights.toggleLighting(false)
        day = true
        lantern.stopLanternEffect()
    }
})
forever(function () {
    music.setVolume(100)
