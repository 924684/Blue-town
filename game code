game.onUpdateInterval(1000, function () {
    if (dosomthing == true) {
        projectile = shader.createImageShaderSprite(img`
            . . 1 1 1 . . . . . . . . . . . 
            . . 1 1 1 . . . . . . . . . . . 
            . . . . . . . . . . . . . . . . 
